keytool -certreq -alias subham -sigalg SHA256withRSA -file subham.csr -keypass changeit -keystore subham.p12 -dname "cn=cts.com,ou=CTS,dc=example,dc=com" -storepass changeit -storetype PKCS12

keytool -gencert -rfc -infile subham.csr -outfile subham.crt -alias subham -sigalg SHA256withRSA -dname CN=subham -startdate "2021/07/12 00:00:00" -validity 365 -keypass changeit -keystore subham.p12 -storepass changeit -storetype PKCS12 -v

